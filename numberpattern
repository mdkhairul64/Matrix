#include <stdio.h>

int l;
int a[15];
int id=0;

void g0(int i,int j){
	if(j==(id-1)){
		if(i==1||i==5){
			printf("*****");
		}
		else{
		printf("*   *");
		}
	}
	else{
		if(i==1||i==5){
			printf("***** ");
		}	
		else{
			printf("*   * ");
		}
	}
}

void g1(int i,int j){
		if(j==(id-1)){
			printf("    *");
		}
		else{
			printf("    * ");
		}
}

void g2(int i,int j){
		if(j==(id-1)){
				if(i&1){
				printf("*****");
			}	
			else{
				if(i==2){
					printf("    *");
				}
				else{
					printf("*    ");
				}
			}
		}
		else{
			if(i&1){
				printf("***** ");
			}
			else{
				if(i==2){
					printf("    * ");
				}
				else{
					printf("*     ");
				}
			}
		}
}
void g3(int i,int j){
		if(j==(id-1)){
			if(i&1){
				printf("*****");
			}
			else{
				printf("    *");
			}
		}
		else{
			if(i&1){
				printf("***** ");
			}
			else{
				printf("    * ");
			}
		}
}
void g4(int i,int j){
		if(j==id-1){
			if(i==3){
				printf("*****");
			}
			else{
				if(i>3){
					printf("    *");
				}
				else{
					printf("*   *");
				}
			}
		}
		else{
			if(i==3){
				printf("***** ");
			}
			else{
				if(i>3){
					printf("    * ");
				}
				else{
					printf("*   * ");
				}
			}
		}
}
void g5(int i,int j){
		if(j==(id-1)){
			if(i&1){
				printf("*****");
			}
			else{
				if(i==2){
					printf("*    ");
				}
				else{
					printf("    *");
				}	
			}
		}
		else{
			if(i&1){
			printf("***** ");
			}
			else{
				if(i==2){
					printf("*     ");
				}
				else{
					printf("    * ");
				}
			}
		}
}
void g6(int i,int j){
		if(j==(id-1)){
			if(i&1){
			printf("*****");
		}
		else{
			if(i==2){
				printf("*    ");
			}
			else{
				printf("*   *");
			}
		}
		}
		else{
			if(i&1){
			printf("***** ");
		}
		else{
			if(i==2){
				printf("*     ");
			}
			else{
				printf("*   * ");
			}
		}
		}
}
void g7(int i,int j){
		if(j==(id-1)){
				if(i==1) printf("*****");
			else
				printf("    *");
			}
		else{
			if(i==1) printf("***** ");
		else
			printf("    * ");
		}
}
void g8(int i,int j){
		if(j==(id-1)){
			if(i&1){
				printf("*****");
			}
			else{
				printf("*   *");
			}
		}
		else{
			if(i&1){
				printf("***** ");
			}
			else{
				printf("*   * ");
			}
		}
}
void g9(int i,int j){
		if(j==(id-1)){
			if(i&1){
				printf("*****");
			}
			else{
				if(i==2){
					printf("*   *");
				}	
				else{
					printf("    *");
				}
			}
		}
		else{
			if(i&1){
				printf("***** ");
			}
			else{
				if(i==2){
					printf("*   * ");
				}
				else{
					printf("    * ");
				}
			}
		}
}
int main(){
	int n;
	char ch;
	while((ch=getchar())!=EOF){
		if(ch>='0'&&ch<='9'){
			n=ch-'0';
			a[id]=n;
			id++;
		}
	}
	for (int i =1; i<=5; ++i)
	{
		for(int j=0;j<id;j++){
			if(a[j]==0){
				g0(i,j);
			}
			if(a[j]==1){
				g1(i,j);
			}
			if(a[j]==2){
				g2(i,j);
			}
			if(a[j]==3){
				g3(i,j);
			}
			if(a[j]==4){
				g4(i,j);
			}
			if(a[j]==5){
				g5(i,j);
			}
			if(a[j]==6){
				g6(i,j);
			}
			if(a[j]==7){
				g7(i,j);
			}
			if(a[j]==8){
				g8(i,j);
			}
			if(a[j]==9){
				g9(i,j);
			}
		}
		printf("\n");
	}
}
